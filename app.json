{
  "expo": {
    "name": "apix-mobile",
    "slug": "apix-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/apix_icon_2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/apix_icon_2.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "package": "com.johnpk1007.apixmobile",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09101d5c-5d3b-4e85-93d4-85b0b210e44d"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/09101d5c-5d3b-4e85-93d4-85b0b210e44d"
    }
  }
}
